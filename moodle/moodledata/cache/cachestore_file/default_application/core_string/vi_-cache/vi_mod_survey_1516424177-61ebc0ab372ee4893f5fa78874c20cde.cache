a:213:{s:6:"actual";s:12:"Hiện tại";s:11:"actualclass";s:24:"Lớp học hiện tại";s:13:"actualstudent";s:17:"{$a} hiện tại";s:12:"allquestions";s:60:"Tất cả câu hỏi theo thứ tự, tất cả sinh viên";s:9:"allscales";s:41:"Tất cả điểm, tất cả sinh viên";s:16:"alreadysubmitted";s:45:"Bạn sẵn sàng nộp bài khảo sát này";s:10:"analysisof";s:22:"Phân tích của {$a}";s:7:"answers";s:16:"Câu trả lời";s:10:"attlsintro";s:442:"Mục đích của những câu hỏi điều tra này là giúp chúng tôi đánh giá các quan điểm của bạn đối với ý kiến và sự học tập.


Không có câu trả lời 'đúng' hoặc 'sai' ; chúng tôi chỉ quan tâm đến quan điểm của bạn. Vui lòng chắc chắn rằng các câu trả lời của bạn được cân nhắc kỹ lưỡng, và sẽ không ảnh hưởng đến đánh giá của bạn.";s:11:"attlsmintro";s:27:"Theo sự thảo luận ...";s:7:"attlsm1";s:52:"Quan điểm theo hướng suy nghĩ và học tập";s:7:"attlsm2";s:26:"Kiến thức mạch lạc";s:7:"attlsm3";s:27:"Kiến thức gián đoạn";s:9:"attlsname";s:31:"ATTLS ( phiên bản 20 mục )";s:6:"attls1";s:112:"Theo đánh giá những gì mà ai đó nói, tôi tập trung vào nội dung chủ đề của họ đưa ra.";s:11:"attls1short";s:50:"Tập trung vào chất lượng của chủ đề";s:7:"attls10";s:95:"Nó quan trọng đối với tôi để giữ nguyên mục tiêu khi phân tích mọi thứ.";s:12:"attls10short";s:25:"Giữ nguyên mục tiêu";s:7:"attls11";s:74:"Tôi cố gắng hiểu mọi người thay vì chống đối lại họ.";s:12:"attls11short";s:21:"Hiểu mọi người";s:7:"attls12";s:104:"Tôi có một số tiêu chuẩn đánh được sử dụng để đánh giá các ý kiến đưa ra.";s:12:"attls12short";s:43:"Sử dụng tiêu chuẩn để đánh giá";s:7:"attls13";s:120:"Tôi hầu như cố gắng để hiểu quan điểm của những ngưới khác hơn là cố gắng đánh giá nó.";s:12:"attls13short";s:24:"Cố gắng để hiều";s:7:"attls14";s:140:"Tôi có gắng chỉ ra điểm yếu trong suy nghĩ của những người khác để giúp họ làm sáng tỏ chủ đề của họ.";s:12:"attls14short";s:27:"Chỉ ra các điểm yếu";s:7:"attls15";s:157:"Tôi đặt mình vào trong hoàn cảnh của người khác khi thảo luận các vấn đề, để xem tại sao họ lại  nghĩ ra cách họ làm .";s:12:"attls15short";s:41:"Đặt mình vào hoàn cảnh của họ";s:7:"attls16";s:190:"Ai đó có thể gọi phương pháp phân tích mọi thứ của tôi  'giả thiết là dùng thử ' bởi vì tôi là người cẩn thận đối với tất cả các vấn đề.";s:12:"attls16short";s:20:"Đưa ra dùng thử";s:7:"attls17";s:125:"Tôi đánh giá tính hợp lý thông qua  sự đồng ý của các đồng nghiệp khi giải quyết các vấn đề.";s:12:"attls17short";s:47:"Tôi đánh giá tính hợp lý nhiều nhất";s:7:"attls18";s:131:"Tôi có thể đạt được sự hiểu biết sâu sắc thông qua các quan điểm dựa vào sự hiểu biết của tôi.";s:12:"attls18short";s:58:"Sự hiểu biết sâu sắc dựa vào sự thấu cảm";s:7:"attls19";s:249:"Khi tôi gặp mọi người mà quan điểm của họ trái ngược với tôi, tôi sẽ cố gắng giải thích cho họ hiểu quan điểm của tôi và tôi cố gắng tìm hiểu xem tại sao họ lại có những quan điểm đó.";s:12:"attls19short";s:41:"Tạo một kết qủa để mở rộng";s:6:"attls2";s:77:"Tôi thích tranh luận ngược lại với những gì ai đó đang nói.";s:11:"attls2short";s:59:"Đùa giỡn với người biện hộ của ngưòi khác";s:7:"attls20";s:148:"Tôi sử dụng thời gian để tìm ra điều gì là sai. Ví dụ tôi tìm mọi thứ theo nghĩa đen mà không đủ để tranh luận.";s:12:"attls20short";s:21:"Điểu sai là gì ?";s:6:"attls3";s:136:"Tôi muốn biết những người khác đến từ đâu, kinh nghiệm gì chỉ huy họ ảnh hưởng đến cách mà họ làm.";s:11:"attls3short";s:32:"Mọi người đến từ đâu";s:6:"attls4";s:122:"Nhiệm vụ quan trọng nhất trong việc học tập của tôi là học tập để hiểu được người khác.";s:11:"attls4short";s:77:"Nhận thức được cách giải thích của những người khác nhau.";s:6:"attls5";s:119:"Tôi nhận thấy rằng cách tốt nhất để tự đánh giá mình là giao tiếp nhiều với mọi người.";s:11:"attls5short";s:32:"Tương tác với nhiều thứ";s:6:"attls6";s:250:"Tôi thích nghe các quan điểm của mọi người đến từ các tầng lớp xã hội khác nhau - điều đó giúp tôi hiểu được những thứ giống nhau như thế nào thì có thể được hiểu theo các cách khác nhau.";s:11:"attls6short";s:29:"Thích nghe các quan điểm";s:6:"attls7";s:150:"Tôi nhận thấy rằng tôi có thể củng cố vị trí của tôi thông qua tranh luận với những người không đồng ý với tôi.";s:11:"attls7short";s:35:"Củng cố bởi sự tranh luận";s:6:"attls8";s:128:"Tôi luôn luôn quan tâm đến việc nhận ra tại sao mọi người nói và tin tưởng những điều mà họ làm.";s:11:"attls8short";s:47:"Nhận biết tại sao mọi người làm nó";s:6:"attls9";s:166:"Tôi thường đặt mình vào để tranh luận với các tác giả của các cuốn sách mà tôi đọc, cố gắng hình dung ra xem tại sao chúng sai.";s:11:"attls9s